<div class="container">
  <div class="jumbotron">
    <h1><%= current_user.name %>'s Dashboard</h1>
  </div>

  <div class="page-header">
    <h1>Recent Patients</h1>
  </div>
  <div class="col-md-6">
    <button class="btn btn-default"><%= link_to "New Patient", new_patient_path %> <span class="glyphicon glyphicon-pencil"></button>
  </div>
  <div class="col-md-6">
    <button class="btn btn-default"><%= link_to "Search Database", patients_path %> <span class="glyphicon glyphicon-list"></span></button>
  </div>
  <br />
  <%= render "patients/patientsindex" %>
</div>
